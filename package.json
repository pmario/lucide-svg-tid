{
	"name": "lucide-to-tid",
	"version": "0.1.0",
	"description": "convert Lucide svgs to .tid usable by TiddlyWiki5",
	"main": "",
	"scripts": {
		"build-tw-plugin": "npm run build:icons && npm run build:outlined && npm run optimize:outlined-tw && npm run make:plugin",
		"build:icons": "node ./scripts/build-icons-from-origin.mjs",
		"build:outlined": "node ./scripts/build-outlines-from-icons.mjs",
		"optimize:outlined-tw": "node ./scripts/optimize-tw-outlined.mjs",
		"make:plugin": "node ./scripts/make-tw-plugin.mjs",
		"test": "echo \"Error: no test specified\" && exit 1",
		"optimize:outlined-lucide": "node ./scripts/optimize-lucide-outlined.mjs",
		"make:plugin-lucide": "node ./scripts/make-lucide-plugin.mjs",
	},
	"keywords": [
		"Lucide",
		"tiddlywiki",
		"svgs",
		"tid"
	],
	"author": "pmario",
	"license": "MIT",
	"dependencies": {
		"svgo": "^3.2.0",
		"svgson": "^5.3.1",
		"prettier": "3.2.4",
		"oslllo-svg-fixer": "^3.0.0"
	}
}
